<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% items.forEach(function(i) { %>
      <tr>
        <td><%= i.title %></td>
        
        <td><%= i.price %></td>

        <td><%= i.currency %></td>

      
        
        <td><a href="/items/<%= i._id %>">DETAILS</a></td>

        <td><form action="/items/<%= i._id %>?_method=DELETE" method="POST">
        <button type="submit">X</button></form></td>

      </tr>
      
      <% }); %>
     
     
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
